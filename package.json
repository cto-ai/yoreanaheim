  "description": "Anaheim Gazetteer",
  "main": "index.js",
  "scripts": {
    "webserver": "node api/src/server.js",
    "test": "node ./node_modules/mocha/bin/mocha --compilers js:babel-core/register --require ./app/tests/test_helper.js \"app/tests/**/*.@(js|jsx)\"",
    "test:watch": "npm run test -- --watch",
    "build": "node_modules/.bin/webpack",
    "dev": "node_modules/.bin/webpack-dev-server",
    "lint": "node ./node_modules/eslint/bin/eslint . --ext .js --ext .jsx",
    "deploy": "set NODE_ENV=production&& webpack -p",
    "clean": "rm -rf app/dist app/build"
  },
